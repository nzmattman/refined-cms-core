{
  "name": "refinedcms",
  "version": "1.5.3",
  "scripts": {
    "dev": "npm run development",
    "development": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "publish-and-tag": "git push --tags && git push origin master",
    "publish-patch": "npm version patch && npm run publish-and-tag",
    "publish-minor": "npm version minor && npm run publish-and-tag",
    "publish-marjor": "npm version major && npm run publish-and-tag"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/refineddigital/cms.git"
  },
  "author": "Matthias Law",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/refineddigital/cms/issues"
  },
  "homepage": "https://gitlab.com/refineddigital/cms#README",
  "devDependencies": {
    "html-loader": "^0.5.5",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "^18.0.0",
    "@ckeditor/ckeditor5-basic-styles": "^18.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^12.0.9",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^8.0.9",
    "@ckeditor/ckeditor5-editor-classic": "^18.0.0",
    "@ckeditor/ckeditor5-essentials": "^18.0.0",
    "@ckeditor/ckeditor5-heading": "^18.0.0",
    "@ckeditor/ckeditor5-horizontal-line": "^18.0.0",
    "@ckeditor/ckeditor5-link": "^18.0.0",
    "@ckeditor/ckeditor5-list": "^18.0.0",
    "@ckeditor/ckeditor5-media-embed": "^18.0.0",
    "@ckeditor/ckeditor5-paragraph": "^18.0.0",
    "@ckeditor/ckeditor5-paste-from-office": "^18.0.0",
    "@ckeditor/ckeditor5-remove-format": "^18.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^18.0.0",
    "@ckeditor/ckeditor5-vue": "^1.0.1",
    "axios": "^0.18.1",
    "dragula": "^3.7.2",
    "dropzone": "^5.5.1",
    "flatpickr": "^4.6.3",
    "javascript-natural-sort": "^0.7.1",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sweetalert": "^2.1.2",
    "vue": "^2.6.10",
    "vue-multiselect": "^2.1.6",
    "vue2-selectize": "^1.1.7"
  }
}
